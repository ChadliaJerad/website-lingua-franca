target TypeScript;

reactor Source {
    output out:string;
    reaction(startup) -> out {=
        out = "Hello World!";
        util.requestStop();
    =}
}
reactor Destination {
    input inp:string;
    reaction(inp) {=
        console.log(`Received: $inp`);
    =}
}

federated reactor Federated {
    s = new Source();
    d = new Destination();
    s.out -> d.inp;
}

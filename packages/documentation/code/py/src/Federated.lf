target Python;

reactor Source {
    output out;
    reaction(startup) -> out {=
        out.set("Hello World!")
        request_stop()
    =}
}
reactor Destination {
    input _in;
    reaction(_in) {=
        print(f"Received {_in.value}")
    =}
}
 
federated reactor Federated {
    s = new Source();
    d = new Destination();
    s.out -> d._in;
}

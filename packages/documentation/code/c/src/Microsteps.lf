target C;
main reactor {
    state count:int(1);
    logical action a;
    reaction(startup, a) -> a {=
        info_print("%d: Logical time is %lld. Microstep is %d.",
            self->count, lf_tag().time, lf_tag().microstep
        );
        if (self->count++ < 5) {
            lf_schedule(a, 0);
        }
    =}
}

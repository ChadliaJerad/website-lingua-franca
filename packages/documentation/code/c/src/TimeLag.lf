target C;
main reactor TimeLag {
    timer t(0, 1 sec);
    reaction(t) {=
        interval_t t = lf_time_logical_elapsed();
        interval_t T = lf_time_physical_elapsed();
        info_print(
            "Elapsed logical time: %lld, physical time: %lld, lag: %lld",
            t, T, T-t
        );
    =}
}

target C;
reactor Schedule {
    input x:int;
    logical action a;
    reaction(x) -> a {=
        lf_schedule(a, MSEC(200));
    =}
    reaction(a) {=
        interval_t elapsed_time = lf_time(LF_ELAPSED_LOGICAL);
        printf("Action triggered at logical time %lld nsec after start.\n", elapsed_time);
    =}
}

target C;
main reactor(p:hello_t("Earth", 42)) {
    preamble {=
        typedef struct hello_t {
            char* name;
            int value;
        } hello_t;
    =}
    reaction(startup) {=
        info_print("Parameter p.name=\"%s\", value=%d.", self->p.name, self->p.value);
        if (self->p.value != 42) {
            error_print_and_exit("Expected 42.");
        }
    =}
}
